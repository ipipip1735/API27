<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/oneFragment">

    <fragment android:id="@+id/oneFragment"
        android:name="mine.material.FadeOneFragment"
        tools:layout="@layout/frgment_fade_one">
        <action
            android:id="@+id/action_oneFragment_to_twoFragment"
            app:destination="@id/twoFragment" />
    </fragment>
    <fragment
        android:id="@+id/twoFragment"
        android:name="mine.material.FadeTwoFragment"
        android:label="TwoFragment"
        tools:layout="@layout/frgment_fade_two"/>

</navigation>